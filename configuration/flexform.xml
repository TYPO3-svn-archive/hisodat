<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>

		<mainSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:hisodat/lang/locallang_flexform.xml:mainSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>

					<controller>
						<TCEforms>
							<label>LLL:EXT:hisodat/lang/locallang_flexform.xml:controllerSelection</label>
							<config>
							<type>select</type>
								<items type="array">
 									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:hisodat/lang/locallang_flexform.xml:controllerSelection.searchForms</numIndex>
										<numIndex index="1">searchForms</numIndex>
									</numIndex>
 									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:hisodat/lang/locallang_flexform.xml:controllerSelection.searchResultList</numIndex>
										<numIndex index="1">searchResultList</numIndex>
									</numIndex>
 									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:hisodat/lang/locallang_flexform.xml:controllerSelection.detailsView</numIndex>
										<numIndex index="1">detailsView</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:hisodat/lang/locallang_flexform.xml:controllerSelection.persons</numIndex>
										<numIndex index="1">persons</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">LLL:EXT:hisodat/lang/locallang_flexform.xml:controllerSelection.localities</numIndex>
										<numIndex index="1">persons</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">LLL:EXT:hisodat/lang/locallang_flexform.xml:controllerSelection.entities</numIndex>
										<numIndex index="1">persons</numIndex>
									</numIndex>
								</items>
 							</config>
						</TCEforms>
					</controller>

					<storageFolder>
						<TCEforms>
							<label>LLL:EXT:hisodat/lang/locallang_flexform.xml:storageFolderSelection</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>5</size>
								<maxitems>22</maxitems>
								<minitems>1</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</storageFolder>

					<recursive>
						<TCEforms>
						<label>LLL:EXT:hisodat/lang/locallang_flexform.xml:recursive</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.2</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.3</numIndex>
										<numIndex index="1">3</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.4</numIndex>
										<numIndex index="1">4</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.5</numIndex>
										<numIndex index="1">250</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</recursive>

				</el>
			</ROOT>
		</mainSheet>

		<searchSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:hisodat/lang/locallang_flexform.xml:searchSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>

					<searchType>
						<TCEforms>
							<label>LLL:EXT:hisodat/lang/locallang_flexform.xml:searchTypeSelection</label>
							<config>
							<type>select</type>
								<items type="array">
 									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:hisodat/lang/locallang_flexform.xml:searchTypeSelection.quickSearchForm</numIndex>
										<numIndex index="1">quickSearchForm</numIndex>
									</numIndex>
 									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:hisodat/lang/locallang_flexform.xml:searchTypeSelection.standardSearchForm</numIndex>
										<numIndex index="1">standardSearchForm</numIndex>
									</numIndex>
 									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:hisodat/lang/locallang_flexform.xml:searchTypeSelection.expertSearchForm</numIndex>
										<numIndex index="1">expertSearchForm</numIndex>
									</numIndex>
 									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:hisodat/lang/locallang_flexform.xml:searchTypeSelection.jump2RecordSearchForm</numIndex>
										<numIndex index="1">jump2RecordSearchForm</numIndex>
									</numIndex>									
								</items>
 							</config>
						</TCEforms>
					</searchType>

					<destinationPid>
						<TCEforms>
							<label>LLL:EXT:hisodat/lang/locallang_flexform.xml:destinationPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</destinationPid>

				</el>
			</ROOT>
		</searchSheet>

		<listSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:hisodat/lang/locallang_flexform.xml:listSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>

					<singlePid>
						<TCEforms>
							<label>LLL:EXT:hisodat/lang/locallang_flexform.xml:singlePid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</singlePid>
					
					<listTemplateFile>
						<TCEforms>
							<label>LLL:EXT:hisodat/lang/locallang_flexform.xml:templateFile</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<default></default>
								<wizards type="array">
									<_PADDING>2</_PADDING>
									<link type="array">
										<type>popup</type>
										<title>Link</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=wizard&amp;act=file</script>
										<params type="array">
											<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
											<allowedExtensions>htm,html,tmpl,tpl</allowedExtensions>
										</params>
										<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
								</config>
						</TCEforms>
					</listTemplateFile>								
				</el>			
			</ROOT>
		</listSheet>

		<singleSheet>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:hisodat/lang/locallang_flexform.xml:singleSheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>

					<backToListPid>
						<TCEforms>
							<label>LLL:EXT:hisodat/lang/locallang_flexform.xml:backToListPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</backToListPid>				

				</el>
			</ROOT>
		</singleSheet>

	</sheets>
</T3DataStructure>
